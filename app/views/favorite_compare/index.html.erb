<h2> compare </h2>



<table class="table">
  <tr>
    <th><%= t "internships.attributes.title" %></th>
    <th><%= t "internships.attributes.orientation" %></th>
    <th><%= t "internships.attributes.programming_language" %></th>
    <th><%= t "internships.attributes.salary" %></th>
    <th><%= t "internships.attributes.company_id" %></th>
    <th><%= t "internships.attributes.living_costs" %></th>
    <th><%= t "internships.attributes.working_hours" %></th>
    <th><%= t "internships.attributes.description" %></th>
    <th><%= t "internships.attributes.recommend" %></th>
    <th><%= t "internships.attributes.rating" %></th>

    <th></th>
  </tr>

<% @internships.each do |internship| %>
  <tr>
    <td><%= internship.title %></td>
    <td><%= internship.orientation.name %></td>
    <td><%= internship.programming_languages.map { |n| n.name } %></td>
    <td><%= internship.salary %></td>
    <td><%= internship.company.name %></td>
    <td><%= internship.living_costs %></td>
    <td><%= internship.working_hours %></td>
    <td><%= internship.description %></td>
    <td><%= internship.recommend %></td>

    <td><%= render_stars(internship.rating, self) %></td>

    <td><%= link_to (t "buttons.show"), internship, class: "btn btn-success" %></td>
  </tr>
<% end %>
</table>