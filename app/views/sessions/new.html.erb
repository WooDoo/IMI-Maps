

<h1>Please Sign In</h1>
<%= form_tag sessions_path do %>
<!-- <div id="login-box" class="field"> -->
    <div id="login-element">
     	<%= label_tag :email %>
     	<%= text_field_tag :email, params[:email] %>
     	<%= label_tag :password %>
     	<%= password_field_tag :password %>
   	 	<%= submit_tag "Log In", class: "btn btn-success" %>
     </div>
<!-- </div> -->
<% end %>